{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "../prisma/prisma-test-environment.ts",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@v1/(.*)$": "<rootDir>/../src/main/app/v1/$1",
    "^@test/(.*)$": "<rootDir>/../test/$1",
    "^@core/(.*)$": "<rootDir>/../src/main/app/core/$1",
    "^@/(.*)$": "<rootDir>/../src/$1"
  }
}
