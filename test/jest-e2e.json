{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@v1/(.*)$": "<rootDir>/../src/main/app/v1/$1",
    "^@test/(.*)$": "<rootDir>/../test/$1",
    "^@core/(.*)$": "<rootDir>/main/app/core/$1"
  }
}
